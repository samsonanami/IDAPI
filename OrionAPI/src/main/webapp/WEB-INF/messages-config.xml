<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="fileNotInCorrectFormatMessage" class="java.lang.String">
        <constructor-arg type="java.lang.String" value="The submitted file is not in correct format"/>
    </bean>

    <bean id="maximumFileSizeMessage" class="java.lang.String">
        <constructor-arg type="java.lang.String" value="The file exceeds the maximum file size"/>
    </bean>

    <bean id="jsonNotInCorrectFormatMessage" class="java.lang.String">
        <constructor-arg type="java.lang.String" value="Json not in correct format"/>
    </bean>

</beans>
