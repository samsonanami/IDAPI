<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <bean id="addressCompare" class="com.fintech.orion.documentverification.common.address.AddressCompare"/>
    <bean id="addressDecoding" class="com.fintech.orion.documentverification.common.address.AddressDecoding"/>

    <bean id="operationAddressComparator"
          class="com.fintech.orion.documentverification.strategy.OperationAddressComparator"/>
    <bean id="operationDateComparator" class="com.fintech.orion.documentverification.strategy.OperationDateComparator"/>
    <bean id="operationStringComparator"
          class="com.fintech.orion.documentverification.strategy.OperationStringComparator"/>

    <bean id="dataValidationStrategyProvider"
          class="com.fintech.orion.documentverification.strategy.DataValidationStrategyProvider"/>

    <bean id="documentVerificationFactory"
          class="com.fintech.orion.documentverification.factory.DocumentVerificationFactory"/>

    <bean id="dataComparator" class="com.fintech.orion.documentverification.factory.DataComparator"/>
    <bean id="idDocFullValidator"
          class="com.fintech.orion.documentverification.factory.IdentificationDocumentFullVerification"/>
    <bean id="addressDocFullValidator"
          class="com.fintech.orion.documentverification.factory.AddressDocumentFullVerification"/>
    <bean id="documentDataValidator" class="com.fintech.orion.documentverification.factory.DataValidations"/>
    <bean id="commonConfigurationFactory" class="com.fintech.orion.documentverification.common.configuration.factory.CommonConfigurationFactory" />

    <bean id="verificationConfigurationMap" class="java.util.HashMap" scope="prototype">
        <constructor-arg>
            <map key-type="java.lang.String" value-type="com.fintech.orion.dto.configuration.VerificationConfiguration">
                <entry key="surname" value-ref="surnameVC"/>
                <entry key="given_names" value-ref="givenNameVC"/>
                <entry key="date_of_birth" value-ref="dateOfBirthVC"/>
                <entry key="place_of_birth" value-ref="placeOfBirthVC"/>
                <entry key="family_name" value-ref="familyNameVC"/>
                <entry key="sex" value-ref="sexVC"/>
                <entry key="parents_given_name" value-ref="parentsGivenNameVC"/>
                <entry key="nationality" value-ref="nationalityVC"/>
                <entry key="address" value-ref="addressVC"/>
            </map>
        </constructor-arg>
    </bean>

    <bean id="surnameVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="STRING"/>
    </bean>
    <bean id="givenNameVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="STRING"/>
    </bean>
    <bean id="dateOfBirthVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="DATE"/>
    </bean>
    <bean id="placeOfBirthVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="STRING"/>
    </bean>
    <bean id="familyNameVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="STRING"/>
    </bean>
    <bean id="parentsGivenNameVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="STRING"/>
    </bean>
    <bean id="sexVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="STRING"/>
    </bean>

    <bean id="nationalityVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="STRING"/>
    </bean>

    <bean id="addressVC" class="com.fintech.orion.dto.configuration.VerificationConfiguration">
        <property name="sameValueComparisonStrategyAcrossMultipleResources" value="ADDRESS"/>
    </bean>

    <bean id="passportMRZDecodingStrategy"
          class="com.fintech.orion.documentverification.common.mrz.PassportMRZDecodingStrategy"/>
    <bean id="drivingLicenseMRZDecodingStrategy"
          class="com.fintech.orion.documentverification.common.mrz.DrivingLicenseMZRDecodingStrategy"/>
    <bean id="irlDrivingLicenseMRZDecodingStrategy" class="com.fintech.orion.documentverification.common.mrz.IrlDrivingLicenseMRZDecodingStrategy"/>
    <bean id="passportMRZValidationStrategy" class="com.fintech.orion.documentverification.common.mrz.ValidatePassPortMRZ"/>
    <bean id="drivingLicenseMRZValidationStrategy"
          class="com.fintech.orion.documentverification.common.mrz.ValidateDrivingLicence"/>
    <bean id="irlDrivingLicenseMRZValidationStrategy" class="com.fintech.orion.documentverification.common.mrz.ValidateIrelandDrivingLicense"/>
    <bean id="passportCheckDigitFormation" class="com.fintech.orion.documentverification.common.checkdigit.PassportCheckDigitFormation"/>

    <bean id="dateComparator" class="com.fintech.orion.documentverification.strategy.OperationDateComparator"/>
    <bean id="addressComparator" class="com.fintech.orion.documentverification.common.address.AddressCompare"/>
    <bean id="addressDecoder" class="com.fintech.orion.documentverification.common.address.AddressDecoding"/>
    <bean id="addressValidator" class="com.fintech.orion.documentverification.common.address.ValidateAddress"/>


    <bean id="idDocCustomValidations" class="java.util.ArrayList">
        <constructor-arg>
            <array value-type="com.fintech.orion.documentverification.custom.CustomValidation">
                <ref bean="genderVerification"/>
                <ref bean="minimumAgeVerification"/>
                <ref bean="mrzCheckDigitValidation"/>
                <ref bean="issuedDateAgeLimitValidation"/>
                <ref bean="dateOfIssueEndYearValidation"/>
                <ref bean="ageLimitValidation"/>

            </array>
        </constructor-arg>
    </bean>

    <bean id="addressDocCustomValidations" class="java.util.ArrayList">
        <constructor-arg>
            <array value-type="com.fintech.orion.documentverification.custom.CustomValidation">
                <ref bean="surNameValidation"/>
                <ref bean="addressValidation"/>
                <ref bean="utilityBillDateEndMonthValidation"/>
            </array>
        </constructor-arg>
    </bean>


    <bean id="documentTypeValidation"
          class="com.fintech.orion.documentverification.custom.common.DocumentTypeValidation">
        <property name="criticalValidation" value="false"/>
        <property name="failedRemarksMessage" value="Document type verification failed"/>
        <property name="successRemarksMessage" value="Documet type verification success"/>
    </bean>
    <bean id="documentVersionValidation"
          class="com.fintech.orion.documentverification.custom.common.DocumentVersionValidation">
        <property name="criticalValidation" value="false"/>
        <property name="failedRemarksMessage" value="Document version verification failed"/>
        <property name="successRemarksMessage" value="Document version verification success"/>
    </bean>
    <bean id="surNameValidation"
          class="com.fintech.orion.documentverification.custom.common.SimpleStringFieldValidator">
        <property name="criticalValidation" value="true"/>
        <property name="ocrExtractionFieldName" value="surname"/>
        <property name="failedRemarksMessage" value="Surname not matching "/>
        <property name="successRemarksMessage" value="Surname match"/>
        <property name="verificationDisplayName" value="Surname Verification"/>
    </bean>

    <bean id="addressValidation" class="com.fintech.orion.documentverification.custom.common.AddressValidation">
        <property name="successRemarksMessage" value="Address matching"/>
        <property name="failedRemarksMessage" value="Address not matching"/>
        <property name="ocrFieldBase" value="address_line"/>
        <property name="addressLineCount" value="3"/>
        <property name="criticalValidation" value="false"/>
    </bean>

    <bean id="minimumAgeVerification" class="com.fintech.orion.documentverification.custom.common.MinimumAgeValidation">
        <property name="criticalValidation" value="false"/>
        <property name="ocrExtractionFieldName" value="date_of_birth"/>
        <property name="minimumAge" value="18"/>
        <property name="failedRemarksMessage" value="Customer is under 18 years old"/>
        <property name="successRemarksMessage" value="Customer is over 18 years"/>
    </bean>
    <bean id="genderVerification" class="com.fintech.orion.documentverification.custom.common.GenderValidation">
        <property name="criticalValidation" value="false"/>
        <property name="ocrExtractionFieldName" value="sex"/>
        <property name="successRemarksMessage" value="Customer is "/>
        <property name="failedRemarksMessage" value="Mismatching gender over multiple documents"/>
    </bean>

    <bean id="dateOfBirthVerification"
          class="com.fintech.orion.documentverification.custom.common.DateOfBirthValidation">
        <property name="criticalValidation" value="true"/>
        <property name="failedRemarksMessage" value="Date of birth is not similar"/>
        <property name="successRemarksMessage" value="Date of birth verified successfully"/>
        <property name="ocrExtractionFieldName" value="date_of_birth"/>
    </bean>

    <bean id="mrzCheckDigitValidation"
          class="com.fintech.orion.documentverification.custom.common.MrzCheckDigitValidation">
        <property name="ocrFieldBase" value="MRZ_Line"/>
        <property name="mrzLineCount" value="2"/>
        <property name="resourceNameToCheckMRZ" value="passport"/>
        <property name="successRemarksMessage" value="MRZ check digit validation successful."/>
    </bean>

    <bean id="ageLimitValidation" class="com.fintech.orion.documentverification.custom.common.AgeLimitValidation">
        <property name="ocrExtractionFieldName" value="date_of_birth"/>
        <property name="criticalValidation" value="false"/>
        <property name="minimumAge" value="18"/>
        <property name="maximumAge" value="90"/>
        <property name="failedRemarksMessage" value="Age limit verification failed"/>
        <property name="successRemarksMessage" value="Customer is between 18 and 90 "/>
    </bean>

    <bean id="utilityBillDateEndMonthValidation"
          class="com.fintech.orion.documentverification.custom.common.BillDateEndMonthValidation">
        <property name="ocrExtractionFieldName" value="utility_bill_date"/>
        <property name="criticalValidation" value="false"/>
        <property name="validMonthCount" value="3"/>
        <property name="failedRemarksMessage" value="Utility bill is older than 3 months"/>
        <property name="successRemarksMessage" value="Utility bill is within 3 months"/>
    </bean>

    <bean id="dateOfIssueEndYearValidation"
          class="com.fintech.orion.documentverification.custom.common.DateOfIssueEndYearValidation">
        <property name="ocrExtractionFieldName" value="date_of_issue"/>
        <property name="criticalValidation" value="false"/>
        <property name="validYearCount" value="10"/>
        <property name="successRemarksMessage" value="Success"/>
        <property name="failedRemarksMessage" value="Failed"/>
    </bean>

    <bean id="issuedDateAgeLimitValidation"
          class="com.fintech.orion.documentverification.custom.common.IssuedDateAgeLimitValidation">
        <property name="ocrExtractionFieldName" value="date_of_issue"/>
        <property name="dateOfBirthOcrExtractionField" value="date_of_birth"/>
        <property name="criticalValidation" value="false"/>
        <property name="minimumAge" value="18"/>
        <property name="maximumAge" value="90"/>
        <property name="failedRemarksMessage" value="failed"/>
        <property name="successRemarksMessage" value="success"/>
    </bean>


    <bean id="dataValidations" class="java.util.ArrayList">
        <constructor-arg>
            <array value-type="com.fintech.orion.documentverification.custom.CustomValidation">
                <ref bean="surnameVizMrzValidation"/>
                <ref bean="dateOfBirthVizMrzValidation"/>
                <ref bean="documentNumberVizMrzValidation"/>
                <ref bean="genderVizMrzValidation"/>
                <ref bean="givenNameVizMrzValidation"/>
                <ref bean="expireDateVizMrzValidation"/>
            </array>
        </constructor-arg>
    </bean>

    <bean id="surnameVizMrzValidation"
          class="com.fintech.orion.documentverification.custom.datavalidation.SurnameVizMrzValidation">
        <property name="ocrExtractionFieldName" value="surname"/>
        <property name="criticalValidation" value="true"/>
        <property name="failedRemarksMessage" value="Failed to validate surname mrz and viz values"/>
        <property name="successRemarksMessage" value="Successfully validated surname mrz and viz values"/>
        <property name="dataValidationStrategyType" value="STRING"/>
        <property name="resourceNames">
            <util:list>
                <value>passport</value>
                <value>drivingLicenseFront</value>
            </util:list>
        </property>
    </bean>

    <bean id="dateOfBirthVizMrzValidation"
          class="com.fintech.orion.documentverification.custom.datavalidation.DateOfBirthVizMrzValidation">
        <property name="ocrExtractionFieldName" value="date_of_birth"/>
        <property name="criticalValidation" value="true"/>
        <property name="failedRemarksMessage" value="Failed to validate date of birth"/>
        <property name="successRemarksMessage" value="date of birth is validated"/>
        <property name="dataValidationStrategyType" value="DATE"/>
        <property name="resourceNames">
            <util:list>
                <value>passport</value>
                <value>drivingLicenseFront</value>
            </util:list>
        </property>
    </bean>

    <bean id="documentNumberVizMrzValidation"
          class="com.fintech.orion.documentverification.custom.datavalidation.DocumentNumberVizMrzValidation">
        <property name="ocrExtractionFieldName" value="passport_number"/>
        <property name="criticalValidation" value="true"/>
        <property name="failedRemarksMessage" value="Failed to validate "/>
        <property name="successRemarksMessage" value="Verified"/>
        <property name="dataValidationStrategyType" value="STRING"/>
        <property name="resourceNames">
            <util:list>
                <value>passport</value>
            </util:list>
        </property>
    </bean>

    <bean id="genderVizMrzValidation"
          class="com.fintech.orion.documentverification.custom.datavalidation.GenderVizMrzValidation">
        <property name="ocrExtractionFieldName" value="sex"/>
        <property name="criticalValidation" value="true"/>
        <property name="failedRemarksMessage" value="Failed to validate "/>
        <property name="successRemarksMessage" value="Verified"/>
        <property name="dataValidationStrategyType" value="STRING"/>
        <property name="resourceNames">
            <util:list>
                <value>passport</value>
            </util:list>
        </property>
    </bean>

    <bean id="givenNameVizMrzValidation"
          class="com.fintech.orion.documentverification.custom.datavalidation.GivenNamesVizMrzValidation">
        <property name="ocrExtractionFieldName" value="given_names"/>
        <property name="criticalValidation" value="true"/>
        <property name="failedRemarksMessage" value="Failed to validate "/>
        <property name="successRemarksMessage" value="Verified"/>
        <property name="dataValidationStrategyType" value="STRING"/>
        <property name="resourceNames">
            <util:list>
                <value>passport</value>
            </util:list>
        </property>
    </bean>

    <bean id="expireDateVizMrzValidation" class="com.fintech.orion.documentverification.custom.datavalidation.ExpireDateVizMrzValidation">
        <property name="ocrExtractionFieldName" value="date_of_expiry"/>
        <property name="criticalValidation" value="true"/>
        <property name="failedRemarksMessage" value="MRZ and VIZ expire date not matching"/>
        <property name="successRemarksMessage" value="MRZ and VIZ expire date matching"/>
        <property name="dataValidationStrategyType" value="DATE"/>
        <property name="resourceNames">
            <util:list>
                <value>passport</value>
            </util:list>
        </property>
    </bean>


    <bean id="documentMrzDecodingConfigurations" class="java.util.ArrayList">
        <constructor-arg>
            <array value-type="com.fintech.orion.documentverification.common.configuration.DocumentMrzDecodingConfigurations">
                <ref bean="passportMrzDecodingConfigurations"/>
                <ref bean="drivingLicenseMrzDecodingConfigurations"/>
            </array>
        </constructor-arg>
    </bean>

    <bean id="passportMrzDecodingConfigurations"
          class="com.fintech.orion.documentverification.common.configuration.DocumentMrzDecodingConfigurations">
        <property name="documentName" value="passport"/>
        <property name="mrzOcrExtractionFieldBase" value="MRZ_line"/>
        <property name="mrzDecodingStrategy" ref="passportMRZDecodingStrategy"/>
        <property name="mrzLineCount" value="2"/>
        <property name="mrzValidationStrategy" ref="passportMRZValidationStrategy"/>
    </bean>

    <bean id="drivingLicenseMrzDecodingConfigurations"
          class="com.fintech.orion.documentverification.common.configuration.DocumentMrzDecodingConfigurations">
        <property name="documentName" value="drivingLicenseFront"/>
        <property name="mrzOcrExtractionFieldBase" value="MRZ_line"/>
        <property name="mrzLineCount" value="1"/>
        <property name="mrzDecodingStrategy" ref="drivingLicenseMRZDecodingStrategy"/>
        <property name="mrzValidationStrategy" ref="drivingLicenseMRZValidationStrategy"/>
    </bean>

    <bean id="irelandDrivingLicenseMrzDecodingConfigurations" class="com.fintech.orion.documentverification.common.configuration.DocumentMrzDecodingConfigurations">
        <property name="documentName" value="drivingLicenseFront"/>
        <property name="mrzOcrExtractionFieldBase" value="MRZ_line"/>
        <property name="mrzLineCount" value="1"/>
        <property name="mrzDecodingStrategy" ref="irlDrivingLicenseMRZDecodingStrategy"/>
        <property name="mrzValidationStrategy" ref="irlDrivingLicenseMRZValidationStrategy"/>
    </bean>

    <!--TEMPLATE NAME MAP-->
    <!--TEMPLATE NAME TO CATEGORY-->

    <bean id="templateNameToCategoryMap" name="templateNameToCategoryMap" class="java.util.HashMap">
        <constructor-arg>
            <map key-type="java.lang.String" value-type="java.lang.String">
                <entry key="DLB_UK" value="IrelandDl" />

                <entry key="DLF_Ireland" value="IrelandDl" />
                <entry key="DLF_UK1" value="CommonDl" />
                <entry key="DLF_Sweden" value="CommonDl" />
                <entry key="DLF_UK2" value="CommonDl" />
                <entry key="DLF_UK3" value="CommonDl" />

                <entry key="ID_Finland" value="IrelandDl" />
                <entry key="ID_Germany" value="IrelandDl" />
                <entry key="ID_Poland" value="IrelandDl" />
                <entry key="ID_Spain" value="IrelandDl" />

                <entry key="P_Ireland" value="CommonPassport" />
                <entry key="P_Ireland2" value="CommonPassport" />
                <entry key="P_UK1" value="CommonPassport" />
                <entry key="P_UK2" value="CommonPassport" />

            </map>
        </constructor-arg>
    </bean>


    <!--CONFIGURATION MAP-->
    <!--TEMPLATE CATEGORY TO CONFIGURATION-->

    <bean id="templateCategoryFactory" class="com.fintech.orion.documentverification.template.category.factory.CommonTemplateCategoryFactory" />

    <bean id="commonTemplateCategory" class="com.fintech.orion.documentverification.template.category.CommonTemplateCategory" scope="prototype">
        <constructor-arg value="DLF_UK1"/>
    </bean>

    <bean id="decodingConfigurationsMap" name="decodingConfigurationsMap" class="java.util.HashMap">
        <constructor-arg>
            <map key-type="java.lang.String" value-type="com.fintech.orion.documentverification.common.configuration.DocumentMrzDecodingConfigurations">
                <entry key="CommonDl" value-ref="drivingLicenseMrzDecodingConfigurations" />
                <entry key="CommonPassport" value-ref="passportMrzDecodingConfigurations" />
                <entry key="IrelandDl" value-ref="irelandDrivingLicenseMrzDecodingConfigurations" />
            </map>
        </constructor-arg>
    </bean>
    
    <bean id="ocrResponseReader" name="defaultOcrResponseReader" class="com.fintech.orion.documentverification.custom.common.OcrResponseReader">
        <constructor-arg>
            <bean id="templateMap" class="com.fintech.orion.documentverification.custom.common.TemplateCategoryMap">
                <constructor-arg>
                    <util:map>
                        <entry key="DLB_UK" value="CommonDateDecoder" />

                        <entry key="DLF_Ireland" value="irelandDLDateDecoder" />
                        <entry key="DLF_Sweden" value="ukDLDateDecoder" />
                        <entry key="DLF_UK1" value="ukDLDateDecoder" />
                        <entry key="DLF_UK2" value="ukDLDateDecoder" />
                        <entry key="DLF_UK3" value="ukDLDateDecoder" />

                        <entry key="ID_Finland" value="CommonDateDecoder" />
                        <entry key="ID_Germany" value="CommonDateDecoder" />
                        <entry key="ID_Poland" value="CommonDateDecoder" />
                        <entry key="ID_Spain" value="CommonDateDecoder" />

                        <entry key="P_Ireland" value="IrelandPassportDateDecoder" />
                        <entry key="P_Ireland2" value="IrelandPassportDateDecoder" />
                        <entry key="P_UK1" value="UKPassportDateDecoder" />
                        <entry key="P_UK2" value="UKPassportDateDecoder" />

                        <entry key="UB_EBS_Savings_Document" value="CommonDateDecoder"/>
                        <entry key="UB_EBS_Savings_Document_2" value="CommonDateDecoder"/>
                        <entry key="UB_EDF_Energy" value="CommonDateDecoder"/>
                        <entry key="UB_Electricity_Bill_Sainsbury" value="CommonDateDecoder"/>
                        <entry key="UB_Electricity_Bill_Southern" value="CommonDateDecoder"/>
                        <entry key="UB_Electricty_Bill_Attantic" value="CommonDateDecoder"/>
                        <entry key="UB_EVO_Energy" value="CommonDateDecoder"/>
                        <entry key="UB_Gas_Statement_BritishGas" value="CommonDateDecoder"/>
                        <entry key="UB_Gas_Statement_EDF" value="CommonDateDecoder"/>
                        <entry key="UB_Gas_Statement_Southern" value="CommonDateDecoder"/>
                        <entry key="UB_Tax_Demand_Notice" value="CommonDateDecoder"/>
                        <entry key="UB_Telephone_Bill_BT" value="CommonDateDecoder"/>
                        <entry key="UB_Water_Bill_ThamesWater" value="CommonDateDecoder"/>
                        <entry key="UB_Water_Bill_Veolia" value="CommonDateDecoder"/>

                        <entry key="DEFAULT" value="CommonDateDecoder"/>

                    </util:map>

                </constructor-arg>
            </bean>
        </constructor-arg>
        <constructor-arg>
            <value type="java.lang.String">DEFAULT</value>
        </constructor-arg>
    </bean>
	    
	<bean id="addressBuilder" class="com.fintech.orion.documentverification.custom.common.AddressBuilder" />

    <!--FILTERS-->

    <bean id="filterFactory" class="com.fintech.orion.documentverification.common.mrz.filter.factory.CommonFilterFactory" />

    <bean id="fillerFilter" class="com.fintech.orion.documentverification.common.mrz.filter.FillerFilter" />


    <!--FEATURE POINTS FOR MRZ-->

    <bean id="filteredMrzFactory" class="com.fintech.orion.documentverification.common.mrz.factory.FilteredMrzFactory" />

    <!--IRELAND DL MRZ FEATURES TO DECODE-->

    <util:list id="irelandDl" value-type="com.fintech.orion.dto.featurepoint.FeaturePoint" >
        <ref bean="irelandDlSurname" />
        <ref bean="irelandDlDrivingLicNum" />
    </util:list>

    <bean id="irelandDlSurname" class="com.fintech.orion.dto.featurepoint.FeaturePoint">
        <constructor-arg index="0" value="2"/>
        <constructor-arg index="1" value="12"/>
        <constructor-arg index="2" value="Surname"/>
    </bean>

    <bean id="irelandDlDrivingLicNum" class="com.fintech.orion.dto.featurepoint.FeaturePoint">
        <constructor-arg index="0" value="18"/>
        <constructor-arg index="1" value="27"/>
        <constructor-arg index="2" value="DrivingLicNum"/>
    </bean>

    <bean id="filteredMrz" class="com.fintech.orion.documentverification.common.mrz.FilteredMrz" scope="prototype">
        <constructor-arg index="0" value=""/>
        <constructor-arg index="1" ref="irelandDl" />
        <constructor-arg index="2" ref="fillerFilter"/>
    </bean>

    <bean id="dateDecoder" class="com.fintech.orion.documentverification.common.date.DateDecoder"/>

    <bean id="UKPassportDateDecoder" class="com.fintech.orion.documentverification.common.date.strategy.UKPassportDateDecodeStrategy"/>

    <bean id="IrelandPassportDateDecoder" class="com.fintech.orion.documentverification.common.date.strategy.IrelandPassportDateDecodeStrategy"/>

    <bean id="ukDLDateDecoder" class="com.fintech.orion.documentverification.common.date.strategy.UKDrivingLicenseDateDecodingStrategy"/>
    <bean id="irelandDLDateDecoder" class="com.fintech.orion.documentverification.common.date.strategy.IrelandDrivingLicenseDateDecodingStrategy"/>
    <bean id="CommonDateDecoder" class="com.fintech.orion.documentverification.common.date.strategy.BasicDateDecodingStrategy">
        <property name="dateFormat" value="dd.MM.yyyy"/>
    </bean>

</beans>