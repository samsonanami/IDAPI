<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="drivingLicenseMRZConfigureList" class="java.util.HashMap">
        <constructor-arg>
            <map key-type="java.lang.String"
                 value-type="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
                <entry key="SurName" value-ref="idSurName"/>
                <entry key="DecadeDigitOfBirthYear" value-ref="idDecadeDigitOfBirthYear"/>
                <entry key="DateofBirthMonth" value-ref="idDateofBirthMonth"/>
                <entry key="DateWithinTheBirthMonth" value-ref="idDateWithinTheBirthMonth"/>
                <entry key="DateofBirthYear" value-ref="idDateofBirthYear"/>
                <entry key="InitialsOfTheFirstName" value-ref="idInitialsOfTheFirstName"/>
                <entry key="MZRLength" value-ref="idMZRLength"/>
            </map>
        </constructor-arg>
    </bean>
    <bean id="idSurName" class="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
        <property name="itemName" value="surname"></property>
        <property name="startIndex" value="1"></property>
        <property name="endIndex" value="5"></property>
    </bean>
    <bean id="idDecadeDigitOfBirthYear" class="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
        <property name="itemName" value="DecadeDigitOfBirthYear"></property>
        <property name="startIndex" value="6"></property>
        <property name="endIndex" value="6"></property>
    </bean>
    <bean id="idDateofBirthMonth" class="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
        <property name="itemName" value="DateofBirthMonth"></property>
        <property name="startIndex" value="7"></property>
        <property name="endIndex" value="8"></property>
    </bean>
    <bean id="idDateWithinTheBirthMonth" class="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
        <property name="itemName" value="DateWithinTheBirthMonth"></property>
        <property name="startIndex" value="9"></property>
        <property name="endIndex" value="10"></property>
    </bean>
    <bean id="idDateofBirthYear" class="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
        <property name="itemName" value="DateofBirthYear"></property>
        <property name="startIndex" value="11"></property>
        <property name="endIndex" value="11"></property>
    </bean>
    <bean id="idInitialsOfTheFirstName" class="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
        <property name="itemName" value="InitialsOfTheFirstName"></property>
        <property name="startIndex" value="12"></property>
        <property name="endIndex" value="13"></property>
    </bean>
    <bean id="idMZRLength" class="com.fintech.orion.documentverification.common.mrz.MRZItemProperty">
        <property name="itemName" value="0"></property>
        <property name="startIndex" value="16"></property>
    </bean>

</beans>